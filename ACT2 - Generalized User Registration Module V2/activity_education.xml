<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Education">

    <Button
        android:id="@+id/button"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:layout_marginTop="30dp"
        android:backgroundTint="#040404"
        android:text="Next"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gradSchool" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="20dp"
        android:text="Name of School"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secBox" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Elementary"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/q1Details"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q1Yes" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Secondary"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/secSchool"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Vocational"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/q4bDetails"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="40dp"
        android:text="College"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="60dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:text="Graduate Studies"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/gradSchool"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:text="Basic Education"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/colBox" />

    <TextView
        android:id="@+id/reportView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:text="Educational Attainment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/secBox"
        android:layout_width="96dp"
        android:layout_height="42dp"
        android:layout_marginTop="15dp"
        android:text="Secondary"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/q1Yes"
        app:layout_constraintTop_toBottomOf="@+id/reportView" />

    <CheckBox
        android:id="@+id/gradBox"
        android:layout_width="96dp"
        android:layout_height="42dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:text="Grad Studies"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reportView" />

    <CheckBox
        android:id="@+id/q1Yes"
        android:layout_width="96dp"
        android:layout_height="42dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Elementary"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reportView" />

    <CheckBox
        android:id="@+id/colBox"
        android:layout_width="85dp"
        android:layout_height="43dp"
        android:layout_marginTop="15dp"
        android:text="College"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/gradBox"
        app:layout_constraintStart_toEndOf="@+id/secBox"
        app:layout_constraintTop_toBottomOf="@+id/reportView" />

    <EditText
        android:id="@+id/q1Details"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/elemCourse"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <EditText
        android:id="@+id/secSchool"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/q4aDetails"
        app:layout_constraintTop_toBottomOf="@+id/q1Details" />

    <EditText
        android:id="@+id/q4bDetails"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/q4cDetails"
        app:layout_constraintTop_toBottomOf="@+id/secSchool" />

    <EditText
        android:id="@+id/colSchool"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/q2Details"
        app:layout_constraintTop_toBottomOf="@+id/q4bDetails" />

    <EditText
        android:id="@+id/gradSchool"
        android:layout_width="145dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/q3Details"
        app:layout_constraintTop_toBottomOf="@+id/colSchool" />

    <EditText
        android:id="@+id/elemCourse"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <EditText
        android:id="@+id/q4aDetails"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elemCourse" />

    <EditText
        android:id="@+id/q4cDetails"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q4aDetails" />

    <EditText
        android:id="@+id/q2Details"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q4cDetails" />

    <EditText
        android:id="@+id/q3Details"
        android:layout_width="138dp"
        android:layout_height="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q2Details" />

</androidx.constraintlayout.widget.ConstraintLayout>