<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BackgroundDetails">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:text="Elementary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        android:text="Basic Education/Degree/Course"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="24dp"
        android:text="Name of School"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:text="Secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="College"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:text="Graduate Studies"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:text="Vocational"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Educational Attainment"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/bdc_elemtxt"
        android:layout_width="130dp"
        android:layout_height="31dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <EditText
        android:id="@+id/bdc_secondarytxt"
        android:layout_width="130dp"
        android:layout_height="31dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bdc_elemtxt" />

    <EditText
        android:id="@+id/bdc_vocationaltxt"
        android:layout_width="130dp"
        android:layout_height="31dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bdc_secondarytxt" />

    <EditText
        android:id="@+id/bdc_collegetxt"
        android:layout_width="130dp"
        android:layout_height="31dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bdc_vocationaltxt" />

    <EditText
        android:id="@+id/bdc_graduatetxt"
        android:layout_width="130dp"
        android:layout_height="31dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bdc_collegetxt" />

    <EditText
        android:id="@+id/sc_elemtxt"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toStartOf="@+id/bdc_elemtxt"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <EditText
        android:id="@+id/sc_secondarytxt"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toStartOf="@+id/bdc_secondarytxt"
        app:layout_constraintTop_toBottomOf="@+id/sc_elemtxt" />

    <EditText
        android:id="@+id/sc_vocationaltxt"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toStartOf="@+id/bdc_vocationaltxt"
        app:layout_constraintTop_toBottomOf="@+id/sc_secondarytxt" />

    <EditText
        android:id="@+id/sc_collegetxt"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toStartOf="@+id/bdc_collegetxt"
        app:layout_constraintTop_toBottomOf="@+id/sc_vocationaltxt" />

    <EditText
        android:id="@+id/sc_graduatetxt"
        android:layout_width="128dp"
        android:layout_height="32dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="8sp"
        app:layout_constraintEnd_toStartOf="@+id/bdc_graduatetxt"
        app:layout_constraintTop_toBottomOf="@+id/sc_collegetxt" />

    <Button
        android:id="@+id/submitBtn"
        android:layout_width="82dp"
        android:layout_height="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:backgroundTint="#3F51B5"
        android:text="Submit"
        android:textColorLink="#9C27B0"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bdc_graduatetxt" />

</androidx.constraintlayout.widget.ConstraintLayout>